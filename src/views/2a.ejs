<!DOCTYPE html>
<html>
<head>
    <title>secure</title>
    <link rel="stylesheet" href="/styles.css">
</head>
<body>
<div class="container">
    <h1>Securing Your Connection...</h1>
    <button onclick="completePage()">Click to Proceed</button>
</div>
<script>
    function completePage() {
        fetch('/interaction/complete', { method: 'POST' })
            .then((response) => {
                if (!response.ok) {
                    throw new Error(`HTTP error! status: ${response.status}`);
                }
                return response.json(); // Parse JSON response
            })
            .then((data) => {
                if (data.redirectUrl) {
                    window.location.href = data.redirectUrl; // Redirect to the provided URL
                } else {
                    console.error('No redirect URL provided in the response.');
                }
            })
            .catch((error) => {
                console.error('Error during completion:', error);
            });
    }
</script>
</body>
</html>
